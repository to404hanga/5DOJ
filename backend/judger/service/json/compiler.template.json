{
	"cmd": [{
		"args": ["%s", "%s", "-o", "%s"],
		"env": ["PATH=/usr/bin:/bin"],
		"files": [{
			"content": ""
		}, {
			"name": "stdout",
			"max": 10240
		}, {
			"name": "stderr",
			"max": 10240
		}],
		"cpuLimit": 10000000000,
		"memoryLimit": 104857600,
		"procLimit": 50,
		"copyIn": {
			"%s": {
				"content": "%s"
			}
		},
		"copyOut": ["stdout", "stderr"],
		"copyOutCached": ["%s"]
	}]
}